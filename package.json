{
  "name": "react-rotary-knob",
  "version": "1.1.8",
  "description": "Skinnable Rotary Knob for React",
  "main": "build/commonjs/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hugozap/react-rotary-knob"
  },
  "keywords": [
    "react",
    "knob",
    "dial",
    "slider",
    "rotary",
    "form"
  ],
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "dependencies": {
    "d3-drag": "^1.2.1",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "prop-types": "^15.6.0",
    "react-svgmt": "^1.1.7",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "test": "jest",
    "start": "webpack --watch",
    "clean": "rimraf build",
    "build": "npm run clean && npm run build:umd && npm run build:commonjs",
    "build:commonjs": "webpack --config=webpack.config.js",
    "build:umd": "webpack --config=webpack.config.umd.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "buildstorybook": "build-storybook -c .storybook -o ./docs/storybook",
    "prepublishOnly": "yarn build && yarn",
    "release": "yarn build && yarn buildstorybook && yarn version && git push origin master"
  },
  "author": {
    "name": "Hugo Zapata"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*-tests.js"
    ]
  },
  "devDependencies": {
    "@storybook/react": "^3.3.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.64.0",
    "jest": "^22.4.3",
    "raw-loader": "^0.5.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.10.1",
    "yarn": "^1.3.2"
  },
  "license": "MIT"
}
